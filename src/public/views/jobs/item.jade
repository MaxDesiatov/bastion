li.job
  - var d = new Date(job.addedTime)
  div.time "#{d.toDateString()} #{d.toTimeString()}"
    div.job_id "#{job._id.toString()}"
    if job.finished
     - var outcomeClass = job.failed ? '.failure' : '.success'
      div(class=".outcome#{outcomeClass}")
        if job.failed
          | &#10008;&nbsp;failure
        else
          | &#10003;&nbsp;success
    else
      i.icon-spinner
  div.job_container= job.log
